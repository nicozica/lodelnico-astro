---
import photoblogData from '../data/photoblog.json';
const photos = photoblogData;
---

<!DOCTYPE html>
<html>
<head>
    <title>Debug - Data Check</title>
</head>
<body>
    <h1>Debug Page</h1>
    <p>Total photos: {photos.length}</p>
    <p>First photo title: {photos[0]?.title || 'No photos found'}</p>
    <pre>{JSON.stringify(photos.slice(0, 2), null, 2)}</pre>
    
    <hr>
    
    <h2>Navigation Test</h2>
    <p><a href="/">← Back to Home</a></p>
    <p><a href="/gallery/1">→ Go to Gallery</a></p>
</body>
</html>
